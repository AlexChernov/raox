    get-item "barber_clients" | check
}

get-button "Execute model barber_clients" | click

get-menu "Model/Export Trace Output/Regular" | click

read-file -uri "workspace:/barber_clients/barber_clients.trc" | equals [read-file -uri "workspace:/etalon/barber_clients.trc.etalon"] | verify-true


