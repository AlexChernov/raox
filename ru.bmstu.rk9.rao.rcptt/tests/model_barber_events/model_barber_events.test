get-menu "Window/Perspective/Open Perspective/Other..." | click
with [get-window "Open Perspective"] {
    get-table | select "Rao"
    get-button OK | click
}

get-menu "File/New/Project..." | click
with [get-window "New Project"] {
    with [get-editbox -after [get-label "Wizards:"]] {
        key-type Enter
        set-text rao
    }
    get-button "Next >" | click
}

with [get-window "New Rao Project"] {
    get-group Templates | get-button "Модель простейшей СМО на событиях" | click
    with [get-editbox -after [get-label "Project name:"]] {
        set-text "barber_events"
    }
    get-button Finish | click
}

with [get-view "Monitoring Configuration" | get-tree] {
    get-item "barber_events" | check
}

get-button "Execute model barber_events" | click

get-menu "Model/Export Trace Output/Regular" | click

read-file -uri "workspace:/barber_events/barber_events.trc" | equals [read-file -uri "workspace:/etalon/barber_events.trc.etalon"] | verify-true


