get-menu "File/New/Project..." | click
with [get-window "New Project"] {
    with [get-editbox -after [get-label "Wizards:"]] {
        key-type Enter
        set-text rao
    }
    get-button "Next >" | click
}

with [get-window "New Rao Project"] {
    get-group Templates | get-button "Модель простейшей СМО" | click
    with [get-editbox -after [get-label "Project name:"]] {
        set-text "barber_simple"
    }
    get-button Finish | click
}

with [get-view "Monitoring Configuration" | get-tree] {
    get-item "barber_simple" | check
}

get-button "Execute model barber_simple" | click

get-menu "Model/Export Trace Output/Regular" | click

read-file -uri "workspace:/barber_simple/barber_simple.trc" | equals [read-file -uri "workspace:/etalon/barber_simple.trc.etalon"] | verify-true



