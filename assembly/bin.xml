<assembly>
	<formats>
		<format>zip</format>
	</formats>
	<id>${rao-version}</id>
	<fileSets>
		<fileSet>
			<directory>${eclipse-path}</directory>
			<outputDirectory>/</outputDirectory>
		</fileSet>
		<fileSet>
			<directory>../ru.bmstu.rk9.rao/target</directory>
			<outputDirectory>dropins</outputDirectory>
			<includes>
				<include>ru.bmstu.rk9.rao*.jar</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>../ru.bmstu.rk9.rao.lib/target</directory>
			<outputDirectory>dropins</outputDirectory>
			<includes>
				<include>ru.bmstu.rk9.rao.lib*.jar</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>../ru.bmstu.rk9.rao.tests/target</directory>
			<outputDirectory>dropins</outputDirectory>
			<includes>
				<include>ru.bmstu.rk9.rao.tests*.jar</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>../ru.bmstu.rk9.rao.ui/target</directory>
			<outputDirectory>dropins</outputDirectory>
			<includes>
				<include>ru.bmstu.rk9.rao.ui*.jar</include>
			</includes>
		</fileSet>
	</fileSets>
</assembly>
